# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Payment Configuration (Stripe)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret

# Email Configuration (Resend)
RESEND_API_KEY=re_your_resend_api_key
FROM_EMAIL=noreply@yourdomain.com

# Google Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_SENTRY=false
VITE_ENABLE_CHAT_SUPPORT=false

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_API_CALLS=false

# Third-party Services (Optional)
VITE_SENTRY_DSN=your_sentry_dsn
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id

# API Endpoints
VITE_API_BASE_URL=/api
VITE_API_TIMEOUT=10000

# Application Configuration
VITE_APP_NAME=PharmaNet
VITE_APP_VERSION=1.0.0
VITE_DEFAULT_CURRENCY=MYR
VITE_SUPPORTED_LANGUAGES=en,my

# Social Links (Optional)
VITE_FACEBOOK_URL=https://facebook.com/yourpage
VITE_INSTAGRAM_URL=https://instagram.com/yourprofile
VITE_TWITTER_URL=https://twitter.com/yourprofile
